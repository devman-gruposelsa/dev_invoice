<odoo>
    <record id="view_fecha_ingreso_wizard" model="ir.ui.view">
        <field name="name">project.task.fecha.ingreso.wizard.form</field>
        <field name="model">project.task.fecha.ingreso.wizard</field>
        <field name="arch" type="xml">
            <form string="Modificar Fecha de Ingreso">
                <group>
                    <field name="fecha_ingreso"/>
                </group>
                <footer>
                    <button string="Guardar" type="object" name="action_apply" class="oe_highlight"/>
                    <button string="Cancelar" class="oe_link" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
